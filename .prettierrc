{
    "trailingComma": "none",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": false,
    "printWidth": 100,
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": ["^(.*).css$", "^@(.*)$", "^[./]", "<THIRD_PARTY_MODULES>"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
